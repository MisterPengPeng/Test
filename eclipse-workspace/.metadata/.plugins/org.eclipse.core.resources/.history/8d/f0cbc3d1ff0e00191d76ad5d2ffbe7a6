package demo;

public class Monkey {
	public static void main(String[] args) {
		long startTime = System.currentTimeMillis();
		int b = 20;
		while(true) {
			if (b % 5 == 1) {
				int c = b - ((b - 1) / 5) - 1;
				if (c % 5 == 2) {
					int d = c - ((c - 2) / 5) - 2;
					if (d % 5 == 3) {
						int e = d - ((d - 3) / 5) - 3;
						if (e % 5 == 4) {
							int f = e - ((e - 4) / 5) - 4;
							if (f % 5 == 0) {
								System.out.println(b);
								break;
							}
						}
					}
				}
			}
			b++;
		}
		long endTime = System.currentTimeMillis();
		long time = endTime - startTime;
		System.out.println("≥Ã–Ú”√ ±:" + time);
	}
}
