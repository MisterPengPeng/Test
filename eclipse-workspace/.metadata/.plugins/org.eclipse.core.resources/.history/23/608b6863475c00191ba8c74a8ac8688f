package itemBank;

import java.util.Scanner;

public class ExaminationRoom {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int n = in.nextInt();
		int m = in.nextInt();
		int[] result = new int[n];
		for (int i = 0; i < m; i++) {
			result[i] = in.nextInt();
		}
		
		int a;
		int b;
		for (int i = 0; i < m; i++) {
			a = in.nextInt();
			b = in.nextInt();
			u(a, b);
		}
		
		in.close();
	}

	private static void u(int a, int b) {
		int tA = f(a);
		int tB = f(b);
		
		if (tA == tB) {
			retun;
		}
	}
	
	private static int f(int i) {
		
	}
}
