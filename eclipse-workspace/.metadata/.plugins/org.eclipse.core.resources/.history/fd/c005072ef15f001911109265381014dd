package itemBank;

import java.util.Scanner;

public class MatchMatch {
	public static void main(String[] args) {
		int n;
		int k;
		int[] dp = new int[100005];
		int[] count = new int[100005];
		Scanner in = new Scanner(System.in);
		
		n = in.nextInt();
		k = in.nextInt();
		int M = 0;
		for (int i = 0; i < n; i++) {
			dp[i] = in.nextInt();
			count[dp[i]]++;
			M = Math.max(M, dp[i]);
		}
		
		in.close();
		
		int answer = 0;
		if (k == 0) {
			for (int i = 0; i <= 100000; i++) {
				if (count[i] > 0) {
					answer++;
				}
			}
		}
	}
}
