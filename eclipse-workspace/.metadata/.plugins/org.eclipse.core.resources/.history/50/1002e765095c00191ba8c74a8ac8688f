package itemBank;

import java.util.Scanner;

public class RootedPlant {
	
	static int m;		//格子的行数
	static int n;		//格子的列数
	static int k;		//接下来有k行数据, 每行两个数a, b代表编号为a的格子和编号为b的格子里的植物合根了
	static int[] result;		//根之间的关系
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		m = in.nextInt();
		n = in.nextInt();
		k = in.nextInt();
		result = new int[m * n];
		for (int i = 0; i < m * n; i++) {
			result[i] = i;
		}
		
		int a;
		int b;
		fo r(int i = 0; i < k; i++) {
			a = in.nextInt() - 1;
			b = in.nextInt() - 1;
			u(a, b);
		}
		in.close();
		
	}
}
