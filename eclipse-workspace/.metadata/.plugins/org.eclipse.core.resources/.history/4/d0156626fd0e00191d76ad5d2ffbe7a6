package demo;

public class Main {
	public static void main(String[] args) {
		String a = "01081522291516170310172431";
		char[] b = a.toCharArray();
		for (int i = 0; i < b.length; i++) {
			if (i != 0 && i % 2 != 0) {
				System.out.print(b[i] + " ");
			} else {
				System.out.print(b[i]);
			}
		}
	}
}
